@model List<Category>


<div class="col-lg-4 col-md-12 col-sm-12 col-xs-12">
    <div class="widget">
        <h2 class="widget-title">Popüler Kategoriler</h2>
        <div class="link-widget">


            @foreach (var item in Model)
            {
                <ul>
                    <li><a href="#">@item.CategoryName <span>@item.CategoryArticleCount</span></a></li>
                </ul>
                <!-- end link-widget -->
            }


        </div>
    </div><!-- end widget -->
</div><!-- end col -->